{
  "name": "Link Plus",
  "description": "This extension add alias and urls to links",
  "version": "1.0",
  "permissions": [
    "tabs", 
    "<all_urls>",
    "contextMenus"
  ],
  "browser_action": {
      "default_title": "Link Plus",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["bg.js"]
  },
  "web_accessible_resources": [
  	  "outline.css"
  ],
  "manifest_version": 2
}
